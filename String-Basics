// String-Basics

#include <iostream>
#include <string>
using namespace std;

int main() {
    // string create
    string str;
    // input
    //cin >> str;
    getline(cin,str);
    // print
    cout << str << endl;
    
    cout << "Length " << str.length() << endl;
    cout << "isEmpty " << str.empty() << endl;
    
    str.push_back('s');
    cout << str << endl;
    
    str.pop_back();
    cout << str;
    
    return 0;
}

